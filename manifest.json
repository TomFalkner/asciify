{
    "manifest_version": 2,
    "name": "ASCII-fy",
    "description": "Turn any old picture into an awesome ASCII picture",
    "version": "0.1",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions":[
       "activeTab",
       "tabs",
       "contextMenus",
       "http://*/"
    ],
    "background": {
        "scripts": ["controller.js"]
    }
}
